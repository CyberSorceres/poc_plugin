{
  "name": "exttest",
  "displayName": "extTest",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "exttest.generateTests",
        "category": "Unit Tests Ext",
        "title": "Generate tests for the currently open File"
      },
      {
        "command": "exttest.showSidebar",
        "category": "Unit Tests Ext",
        "title": "Show Sidebar"
      },
      {
        "command": "exttest.runTests",
        "category": "Unit Tests Ext",
        "title": "Run all tests"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.18",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "aws-sdk": "^2.1549.0",
    "aws-sdk-mock": "^5.9.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
